{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build blobstore",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/az-blobstore/azure-blobstore.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/az-blobstore/azure-blobstore.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/az-blobstore/azure-blobstore.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "build storage account table",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/storage-account-tables/storage-account-tables.csproj"
            ],
            "problemMatcher": "$tsc"
        }        
    ]
}