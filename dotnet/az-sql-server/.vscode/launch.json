    "version": "0.2.0",
    "configurations": [
        {
            "name": "az-sql-server-console (debug)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/az-sql-server-console/bin/Debug/netcoreapp2.2/az-sql-server-console.dll",
            "args": [],
            "cwd": "${workspaceFolder}/az-sql-server-console",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}